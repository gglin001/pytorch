{
  "version": 8,
  "configurePresets": [
    {
      "name": "osx_allen",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build",
      "inherits": "osx",
      "environment": {
        "SYCL_ROOT": "/repos_osx/_work/llvm-dpcpp-sycl-web/build/install"
      },
      "cacheVariables": {
        "USE_XPU": true,
        "USE_CUDA": false,
        "USE_KINETO": false,
        "USE_PYTORCH_QNNPACK": false,
        "USE_FBGEMM": false,
        "USE_NNPACK": false,
        "USE_XNNPACK": false,
        "USE_DISTRIBUTED": false,
        "BUILD_LAZY_TS_BACKEND": false,
        //
        "CMAKE_C_COMPILER": "/usr/bin/gcc",
        "CMAKE_CXX_COMPILER": "/usr/bin/g++",
        "SYCL_COMPILER": "/repos_osx/_work/llvm-dpcpp-sycl-web/build/install/bin/clang++",
        "SYCL_EXECUTABLE": "/repos_osx/_work/llvm-dpcpp-sycl-web/build/install/bin/clang++",
        //
        "SYCL_VERBOSE_BUILD": true,
        "CMAKE_VERBOSE_MAKEFILE": true,
        "AA": "BB"
      }
    }
  ]
}
