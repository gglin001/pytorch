{
    // used in docker container
    // https://github.com/gglin001/Dockerfiles/blob/master/clangdev/Dockerfile
    // Do more things
    // conda install glog=0.4.0 -y
    //
    "cmake.configureSettings": {
        "CMAKE_PREFIX_PATH": "/opt/conda/envs/py38",
        "CMAKE_SYSROOT": "/opt/conda/envs/py38/x86_64-conda-linux-gnu/sysroot",
        "CMAKE_POSITION_INDEPENDENT_CODE": "ON",
        "CMAKE_INSTALL_PREFIX": "${workspaceFolder}/torch",
        // https://github.com/vadimcn/vscode-lldb/issues/415
        "CMAKE_C_FLAGS": "-fno-limit-debug-info",
        // "CMAKE_CXX_FLAGS": "-fno-limit-debug-info",
        // workaroud for clang-14
        "CMAKE_CXX_FLAGS": "-fno-limit-debug-info -Wno-error=bitwise-instead-of-logical",
        // "BUILD_CAFFE2": "OFF",
        // "BUILD_CAFFE2_OPS": "OFF",
        // "BUILDING_WITH_TORCH_LIBS": "OFF",
        // "USE_CUDA": "OFF",
        // "USE_ROCM": "OFF",
        // "USE_FBGEMM": "ON",
        // "USE_FAKELOWP": "OFF",
        // "USE_QNNPACK": "ON",
        // "USE_KINETO": "OFF",
        // "USE_NATIVE_ARCH": "OFF",
        // "USE_NNPACK": "OFF",
        "USE_MKLDNN": "OFF",
        // "USE_TENSORPIPE": "OFF",
        // "USE_XNNPACK": "OFF",
        // "USE_GLOO": "OFF",
        // "USE_DISTRIBUTED": "ON",
        // // need glog v0.4.0
        "USE_GLOG": "ON",
        "BUILD_PYTHON": "ON",
        // "USE_OPENMP": "OFF",
        // "BUILD_TEST": "OFF",
        "BUILD_TEST": "ON",
        // "USE_LLVM": "ON",
        "USE_LLVM": "OFF",
        "BUILD_LAZY_TS_BACKEND": "ON",
        "PYTHON_EXECUTABLE": "/opt/conda/envs/py38/bin/python",
    },
    "[cpp]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "python.linting.flake8Enabled": true,
    "python.analysis.extraPaths": [
        "${workspaceFolder}",
    ],
    "terminal.integrated.env.linux": {
        "PYTHONPATH": "${workspaceFolder}",
        "GLOG_v": "10",
        "PYTORCH_JIT_LOG_LEVEL": ">>graph_helper:>>graph_fuser:>>kernel:>>interface",
        "PYTORCH_JIT_TYPE_VERBOSITY": "3",
        // "LTC_SAVE_TENSORS_FILE": "LTC_SAVE_TENSORS_FILE.txt",
    },
    "clangd.path": "/opt/conda/envs/py38/bin/clangd",
}
