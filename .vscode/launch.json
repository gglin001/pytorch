    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Launch",
            "cwd": "${workspaceFolder}",
            // "terminal": "integrated",
            "terminal": "console",
            "program": "${workspaceFolder}/build/bin/test_lazy",
            "args": [
                // "--gtest_filter=*TestMul",
                "--gtest_filter=*TestLogSigmoidBackward",
            ],
            "initCommands": [
                "settings set target.disable-aslr false",
                // "command source ${workspaceRoot}/.lldbinit",
                // "settings set target.process.thread.step-avoid-regexp (.)*c10(.)*",
                // "settings set target.process.thread.step-avoid-regexp (.)lazy(.)*",
            ],
            "env": {
                "GLOG_v": "10",
                "PYTORCH_JIT_LOG_LEVEL": ">>graph_helper:>>graph_fuser:>>kernel:>>interface",
                "PYTORCH_JIT_TYPE_VERBOSITY": "3",
            },
        },
        // {
